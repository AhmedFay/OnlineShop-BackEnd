@model IEnumerable<OnlineSHProject.Models.ApplicationUser>

@{
    ViewBag.Title = "UserManage";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>UserManage</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.FirstN)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastN)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PhoneNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserName)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FirstN)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastN)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PhoneNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.ActionLink("Approve", "", new { id = item.Id }) |
                @Html.ActionLink("Reject", "", new { id = item.Id }) |
                @Html.ActionLink("Details", "", new { id = item.Id })
            </td>
        </tr>
    }

</table>
